package day0325;

//상속
class AA{
	AA()
	{
		System.out.println("AA 의 디폴트 생성자");
	}
}

class BB extends AA
{
	BB()
	{
		super();//생략되어있음,부모의 디폴트 생성자를 호출
		System.out.println("BB 의 디폴트 생성자");
	}
}

public class Ex4_Inheri {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		BB b=new BB();
	}

}
