package day0325;

public class Employee {
	private String name;
	private String position;
	private int gibon;
	private int sudang;
	private int age;	

	//생성자
	public Employee() {
		super();
	}

	public Employee(String name, String position, int age) {
		super();
		this.name = name;
		this.position = position;
		this.age = age;
	}

	public Employee(String name, int age) {
		super();
		this.name = name;
		this.age = age;
	}
	
	
	
	//setter & getter
	
	//추가 메서드들
	
}
